<?xml version="1.0"?>
<odoo>

    <record id="view_list_worker" model="ir.ui.view">
	  <field name="name">Worker List</field>
	  <field name="model">waste.worker</field>
	  <field name="arch" type="xml">
		  <tree>
				<field name="name"/>
				<!--field name="product_code"/>
				<field name="date_published"/>
				<field name="material_type"/>
				<field name="process"/>
				<field name="contract"/>
				<field name="author_ids" widget="many2many_tags"/-->
				<!--field name="image" widget="image"/-->
		  </tree>
	  </field>
	</record>


    <record id="view_form_worker" model="ir.ui.view">
	  <field name="name">User Form</field>
	  <field name="model">waste.worker</field>
	  <field name="arch" type="xml">
		  <form string="Worker">
			<sheet>
				<group name="group_top">
				  <group name="group_left">
					<field name="name"/>
					<!--field name="product_code"/>
					<field name="date_published"/>
					<field name="material_type"/>
					<field name="author_ids" widget="many2many_tags"/-->
				  </group>
				</group>
			</sheet>
		  </form>
	  </field>
	</record>


</odoo>